            <string>///Underwater ripple effect

//Radius of effect
var ir = -4;

//Left boundary
var ix = view_xview[view_current]-16;

//Top boundary
var iy = ystart;

//Width
var iw = 264;

//Height
var ih = 224;

//Draw only if the y position is above the bottom boundary
if (iy &lt; view_yview+view_hview) {

    //Surface
    sf = surface_create(iw, ih);
    
    //Copy the surface
    surface_copy_part(sf, 0, 0, application_surface, ix-view_xview, iy-view_yview, iw, ih);
    
    //Disable alpha blending
    draw_enable_alphablend(false);
    
    //Draw part of the surface
    for (i=0; i&lt;ih; i++) {
    
        draw_surface_part_ext(sf, 0, i, iw, 1, ix-8+cos((i*3+waverot)*pi/180)*ir, iy+i, 1.08, 1, c_white, 1);
    }
    
    //Free surface
    surface_free(sf);
    
    //Enable alpha blending
    draw_enable_alphablend(true);
}
</string>
